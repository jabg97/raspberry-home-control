<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>{% block title %}Welcome!{% endblock %}</title>
       
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Disable tap highlight on IE -->

    <meta name="msapplication-tap-highlight" content="no">

    <!-- Progressive Web App -->

    <meta name="theme-color" content="#f5f5f5">

    <link rel="manifest" href="{{ asset('manifest.json') }}">

    <!-- Add to homescreen for Chrome on Android -->

    <meta name="mobile-web-app-capable" content="yes">

    <link rel="icon" sizes="192x192" href="{{ asset('img/icons/desktop/android-chrome-192x192.png') }}">

    <link rel="icon" sizes="512x512" href="{{ asset('img/icons/desktop/android-chrome-512x512.png') }}">

    <!-- Add to homescreen for Safari on iOS -->

    <meta name="apple-mobile-web-app-capable" content="yes">

    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <meta name="apple-mobile-web-app-title" content="App">

    <link rel="apple-touch-icon-precomposed" href=".{{ asset('img/icons/desktop/apple-touch-icon.png') }}">

    <link rel="mask-icon" href="{{ asset('img/icons/desktop/safari-pinned-tab.svg') }}" color="#5bbad5">

    <!-- Tile color for Windows -->

    <meta name="msapplication-TileImage" content="{{ asset('img/icons/desktop/mstile-150x150.png') }}">

    <meta name="msapplication-TileColor" content="#ffc40d">

    <!-- Place favicon.ico in the root directory -->

    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('mg/favicon/favicon2.png') }}">

    <link rel="icon" type="image/png" sizes="16x16" href=".{{ asset('img/favicon/favicon.png') }}">

        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        
        
        <link rel="stylesheet" type="text/css"  href="{{ asset('css/bootstrap.css') }}"/>
        <link rel="stylesheet" type="text/css"  href="{{ asset('css/mdb.css') }}"/>
        <link rel="stylesheet" type="text/css"  href="{{ asset('css/font-awesome.min.css') }}"/>
        {% block stylesheets %}{% endblock %}       
    </head>
    <body>
        {% block body %}
        {% endblock %}
        <script type="text/javascript" src="{{ asset('js/jquery-3.1.1.min.js') }}" charset="utf-8"></script>   
        <script type="text/javascript" src="{{ asset('js/tether.min.js') }}" charset="utf-8"></script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}" charset="utf-8"></script>
        <script type="text/javascript" src="{{ asset('js/mdb.min.js') }}" charset="utf-8"></script>
        <script type="text/javascript" src="{{ asset('js/sweetalert2.all.js') }}" charset="utf-8"></script>
        <script>
			
        if ('serviceWorker' in navigator) {
			
			var url = "{{ asset('sw.js') }}"
			

		const p = navigator.serviceWorker

			.register(url)

			.then(reg => console.log('PWA service-worker ready.', reg))

			.catch(err => console.error('Could not load service-worker.', err));

	}
</script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
